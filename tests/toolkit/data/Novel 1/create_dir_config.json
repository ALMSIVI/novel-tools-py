{
  "objects": {
    "readers": [
      {
        "class": "MetadataJsonReader",
        "metadata_filename": "../metadata.json"
      },
      {
        "class": "DirectoryReader",
        "read_contents": true,
        "discard_chapters": false
      }
    ],
    "matchers": [
      {
        "class": "CsvMatcher",
        "csv_filename": "../list.csv",
        "types": {
          "filename": "str"
        },
        "fields": ["filename"]
      }
    ],
    "writers": [
      {
        "class": "MarkdownWriter",
        "use_title": true
      }
    ]
  }
}
