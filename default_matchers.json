{
  "volumes": [
    {
      "class": "NumberedMatcher",
      "regex": "^第.+卷.*$",
      "start_index": 1,
      "end_char": "卷"
    },
    {
      "class": "SpecialMatcher",
      "names": ["前传", "最终卷", "后记", "番外篇"]
    }
  ],
  "chapters": [
    {
      "class": "NumberedMatcher",
      "regex": "^第.+章.*$",
      "start_index": 1,
      "end_char": "章"
    },
    {
      "class": "SpecialMatcher",
      "names": ["序章", "楔子", "终章", "番外"]
    }
  ]
}
